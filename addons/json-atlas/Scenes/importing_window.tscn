[sub_resource type="GDScript" id="GDScript_5brun"]
resource_name = "Rich Zoom"
script/source = "@tool
extends RichTextLabel

func _gui_input(event: InputEvent) -> void:
	if event is InputEventMouseButton:
		if event.is_pressed() && event.button_index == MOUSE_BUTTON_WHEEL_UP && event.ctrl_pressed:
			add_theme_font_size_override(\"bold_font_size\", min(get_theme_font_size(\"bold_font_size\") + 2, 32))
			add_theme_font_size_override(\"bold_italics_font_size\", min(get_theme_font_size(\"bold_font_size\") + 2, 32))
			add_theme_font_size_override(\"italics_font_size\", min(get_theme_font_size(\"bold_font_size\") + 2, 32))
			add_theme_font_size_override(\"mono_font_size\", min(get_theme_font_size(\"bold_font_size\") + 2, 32))
			add_theme_font_size_override(\"normal_font_size\", min(get_theme_font_size(\"bold_font_size\") + 2, 32))
		if event.is_pressed() && event.button_index == MOUSE_BUTTON_WHEEL_DOWN && event.ctrl_pressed:
			add_theme_font_size_override(\"bold_font_size\", max(get_theme_font_size(\"bold_font_size\") - 2, 8))
			add_theme_font_size_override(\"bold_italics_font_size\", max(get_theme_font_size(\"bold_font_size\") - 2, 8))
			add_theme_font_size_override(\"italics_font_size\", max(get_theme_font_size(\"bold_font_size\") - 2, 8))
			add_theme_font_size_override(\"mono_font_size\", max(get_theme_font_size(\"bold_font_size\") - 2, 8))
			add_theme_font_size_override(\"normal_font_size\", max(get_theme_font_size(\"bold_font_size\") - 2, 8))
"

